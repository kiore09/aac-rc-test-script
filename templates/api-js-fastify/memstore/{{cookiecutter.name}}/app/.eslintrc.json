{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "valid-jsdoc": "off",
        "new-cap": "off",
        "require-jsdoc" : "off",
        "max-len": "off"
    }
}
