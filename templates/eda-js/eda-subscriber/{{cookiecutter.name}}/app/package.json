{
  "name": "starter-kit-pubsub",
  "version": "1.0.0",
  "description": "GCP PubSub Sample in Nodejs",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src/**/*.js",
    "lint-fix": "eslint --fix src/**/*.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test-coverage": "npm run test -- --coverage --coverageReporters='json-summary' --coverageReporters='lcov'",
    "start": "node src/index.js",
    "start:dev": "nodemon src/index.js",
    "docs": "node swaggerGenerate.js"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@google-cloud/pubsub": "^4.4.1",
    "@google-cloud/firestore": "^7.8.0",
    "express": "^4.18.2",
    "config": "^3.3.7",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "ejs": "^3.1.8",
    "swagger-jsdoc": "^6.2.5",
    "swagger-ui-express": "^5.0.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "eslint": "^9.9.1",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-jsdoc": "^50.2.2",
    "jest": "^29.7.0",
    "nodemon": "^3.1.4"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "engines": {
    "node": "^20"
  }
}
