{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": [
    "google", 
    "plugin:@typescript-eslint/recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint", 
    "react"
  ],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "valid-jsdoc": "off",
    "new-cap": "off",
    "require-jsdoc": "off",
    "max-len": "off"
  }
}
