{
  "name": "sample-graphql-node-server",
  "version": "1.0.0",
  "description": "Sample Node.js app using GCP CloudSQL and Graphql in TypeScript",
  "main": "index.js",
  "author": "TELUS",
  "scripts": {
    "lint": "eslint src/**/*.ts",
    "lint-fix": "eslint --fix src/**/*.ts",
    "test": "jest --config=jest.config.ts",
    "test:watch": "jest --watch",
    "test-coverage": "npm run test -- --coverage --coverageReporters='json-summary' --coverageReporters='lcov'",
    "start": "node ./dist/index.js",
    "build": "tsc && ts-node copy-assets.ts",
    "start:dev": "nodemon src/index.ts",
    "docs": "ts-node swaggerGenerate.ts",
    "dev": "ts-node-dev --respawn --pretty --transpile-only ./src/index.ts"
  },
  "dependencies": {
    "@google-cloud/secret-manager": "^5.4.0",
    "@graphql-tools/schema": "^10.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "ejs": "^3.1.8",
    "express": "^4.18.2",
    "graphql": "16.9.0",
    "graphql-http": "^1.21.0",
    "graphql-playground-middleware-express": "^1.7.23",
    "knex": "^3.1.0",
    "node-fetch": "^3.2.10",
    "pg": "^8.7.3",
    "qs": ">=6.0.4",
    "swagger-jsdoc": "^6.2.5",
    "swagger-ui-express": "^5.0.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/ejs": "^3.1.5",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.5.4",
    "@types/qs": "^6.9.15",
    "@types/shelljs": "^0.8.15",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^8.5.0",
    "@typescript-eslint/parser": "^8.5.0",
    "eslint": "^8.57.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-jsdoc": "^50.2.2",
    "eslint-plugin-react": "^7.35.2",
    "jest": "^29.7.0",
    "nodemon": "^3.1.4",
    "shelljs": "^0.8.5",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.4"
  },
  "engines": {
    "node": "^20"
  }
}
